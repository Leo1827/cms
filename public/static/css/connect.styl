body
	background-color #f6f6f6
	margin 0px
	padding 10px
	
.mtop16 
	margin-top 16px

.ino 
	height 25px
    
.box
	background-color #fff 
	border-radius 8px
	border 1px solid #d6d6d6
	position absolute
	width 350px
	.btn-success
		width 100%
		padding-top 10px
		background-color #7A96A9
		border 0px
	
	.form-control
		&:focus
		&:active
			outline none
			box-shadow none
			
	.inside 
		padding 16px
	.header
		background-color #7A96A9
		padding 16px
		border-radius 8px 8px 0px 0px
		a 
			img
				display block
				margin 0px auto 
				width 50%
	.footer 
		text-align center
		a 
			display inline-block			

.box_login
	left calc(50% - 180px)
	top calc(50% - 196px)
	
.box_register
	left calc(50% - 180px)
	top calc(50% - 317px)